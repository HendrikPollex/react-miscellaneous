{
    "name": "react-miscellaneous",
    "version": "1.0.0",
    "description": "Misc components for react web apps.",
    "author": "Hendrik Pollex",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "!node_modules"
    ],
    "scripts": {
        "clean": "rimraf dist/",
        "copy": "copyfiles -u 1 src/**/*.svg src/**/*.css dist/",
        "build": "tsc && copyfiles -u 1 src/**/*.svg src/**/*.css src/*.css dist/",
        "watch": "tsc -w",
        "storybook": "storybook dev -p 6006 --debug-webpack",
        "build-storybook": "storybook build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HendrikPollex/react-miscellaneous"
    },
    "peerDependencies": {
        "formik": "^2.4.6",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.2",
        "@storybook/addon-essentials": "^8.4.5",
        "@storybook/addon-interactions": "^8.4.5",
        "@storybook/addon-links": "^8.4.5",
        "@storybook/blocks": "^8.4.5",
        "@storybook/preset-create-react-app": "8.4.5",
        "@storybook/react": "^8.4.5",
        "@storybook/react-webpack5": "^8.4.5",
        "@storybook/test": "8.4.5",
        "@storybook/theming": "^8.4.5",
        "@types/node": "^16.18.108",
        "@types/react": "^18.3.5",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/parser": "5.62.0",
        "copyfiles": "^2.4.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-storybook": "^0.11.1",
        "prop-types": "^15.8.1",
        "react-docgen-typescript-plugin": "^1.0.8",
        "rimraf": "^5.0.5",
        "storybook": "^8.4.5",
        "storybook-dark-mode": "^4.0.2",
        "typescript": "^4.9.5"
    }
}
